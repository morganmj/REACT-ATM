{
    "extends": "airbnb",
    "rules": {
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
        "import/no-extraneous-dependencies": [0],
        "linebreak-style": ["error", "unix"],
        "react/prop-types": ["error", { "ignore": ["children", "route"] }],
        "new-cap": [0],
        "react/require-extension": "off"
    },
    "env": {
        "browser": true
    }

}